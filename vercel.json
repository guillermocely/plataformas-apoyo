{
  "functions": {
    "materiales/**": {
      "includeFiles": "materiales/**"
    }
  },
  "rewrites": [
    {
      "source": "/materiales/documentos/(.*)",
      "destination": "/materiales/documentos/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
